@model User
@{
    ViewData["Title"] = "_Navbar";
}



@if (Model == null)
{
    <li><a asp-area="" asp-controller="Account" asp-action="Register">Register</a></li>
    <li><a asp-area="" asp-controller="Account" asp-action="Login">Login</a></li>
}
else
{
    <li><a asp-area="" asp-controller="Account" asp-action="Inbox">Inbox: @Model.ListOfMessages.Count Messages</a></li>
    <li><a asp-area="" asp-controller="Account" asp-action="PerspectiveDates">Find a Match!</a></li>
    <li><a asp-area="" asp-controller="Account" asp-action="BioPage">Welcome @Model.Username</a></li>
    <li><a asp-area="" asp-controller="Account" asp-action="Logoff">Logout</a></li>
}