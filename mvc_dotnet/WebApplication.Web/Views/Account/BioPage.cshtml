@using WebApplication.Web.Models.Account;
@model RegisterViewModel
@{
    ViewData["Title"] = "BioPage";
}
<img class="bg-BioPage" src="https://pixabay.com/illustrations/music-melody-old-fashioned-1363069/">
<div id="push"></div>
<div class="container body-content">
    <div class="transparent-overlay col-xs-11 col-sm-7 col-md-4 col-lg-4">
        <div class="center-form">
            <h2>Bio Page</h2>
            <div class="container body-content">
                <form asp-controller="account" asp-action="EditProfile" method="post">
                    <div class="form-group">
                        <label asp-for="SelfDescription"></label><br />
                        <input type="text" asp-for="SelfDescription" />
                        <span asp-validation-for="SelfDescription"></span>
                    </div>
                    <input type="submit" value="Submit" class="btn btn-default" />
                </form>
            </div>
        </div>
    </div>
</div>
